<%= form_for(current_user, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>

  <div class="field">
    <%= f.label :wallet_address %><br />
    <%= f.text_field :wallet_address %>
  </div>

  <div class="actions">
    <%= f.submit "Update" %>
  </div>

<% end %>